{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prisma:generate": "prisma generate",
    "prisma:test:generate": "prisma generate --schema=prisma/schema.test.prisma",
    "prisma:test:push": "prisma db push --schema=prisma/schema.test.prisma",
    "test:unit:frontend": "cross-env VITEST_MODE=frontend vitest run test/unit/frontend",
    "test:unit:backend": "cross-env VITEST_MODE=backend vitest run test/unit/backend",
    "test:integration:backend": "cross-env VITEST_MODE=integration dotenv -e .env.test vitest run test/integration/backend",
    "test": "npm run test:unit:frontend && npm run test:unit:backend && npm run test:integration:backend"
  },
  "dependencies": {
    "@nuxtjs/supabase": "^1.5.2",
    "@prisma/client": "^6.10.1",
    "@samk-dev/nuxt-vcalendar": "^1.0.4",
    "animate.css": "^4.1.1",
    "canvg": "^4.0.3",
    "chart.js": "^4.5.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "magic-regexp": "^0.10.0",
    "nuxt": "^3.17.5",
    "vue": "^3.5.16",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "6.11.4",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "10.4.20",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "happy-dom": "^20.0.11",
    "postcss": "8.4.47",
    "prisma": "^6.10.1",
    "tailwindcss": "3.4.13",
    "vitest": "^4.0.16"
  }
}
