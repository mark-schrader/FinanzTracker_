generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model categories {
  id       Int     @id @default(autoincrement())
  name     String
  type     String
  user_id  Int?
  icon     String?
  color    String?
}

model incomes {
  id          Int @id @default(autoincrement())
  amount      Float
  category_id Int?
}

model expenses {
  id          Int @id @default(autoincrement())
  amount      Float
  category_id Int?
}
